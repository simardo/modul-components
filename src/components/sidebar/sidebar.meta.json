{
    "tag": "m-sidebar",
    "name": "m-sidebar-meta:name",
    "production": false,
    "attributes": {
        "origin": {
            "type": "string",
            "value": [
                "right",
                "bottom",
                "left",
                "top"
            ],
            "default": 1
        },
        "width": {
            "type": "string"
        },
        "title": {
            "type": "string"
        },
        "closeButton": {
            "type": "boolean",
            "default": true
        },
        "closeButtonPosition": {
            "type": "string",
            "value": [
                "right",
                "left"
            ],
            "default": 0
        },
        "focusManagement": {
            "type": "boolean",
            "default": true
        },
        "closeOnBackdrop": {
            "type": "boolean",
            "default": true
        },
        "padding": {
            "type": "boolean",
            "default": true
        },
        "paddingHeader": {
            "type": "boolean",
            "default": true
        },
        "paddingBody": {
            "type": "boolean",
            "default": true
        },
        "paddingFooter": {
            "type": "boolean",
            "default": true
        },
        "openTrigger": {
            "type": "string",
            "value": [
                "click",
                "hover",
                "manual"
            ],
            "default": 0
        },
        "open": {
            "type": "boolean",
            "default": false
        },
        "id": {
            "type": "string",
            "default": "mPortal"
        },
        "disabled": {
            "type": "boolean",
            "default": false
        },
        "trigger": {
            "type": "HTMLElement"
        }

    },
    "overview": "sidebar.overview",
    "preview": "sidebar.preview"
}
