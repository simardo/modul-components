{
    "tag": "m-timepicker",
    "name": "m-timepicker-meta:name",
    "production": false,
    "attributes": {
        "label": {
            "type": "string"
        },
        "duration": {
            "type": "boolean",
            "default": false
        },
        "time": {
            "type": "moment.Moment | moment.Duration",
            "default": "moment() | moment.duration('1:0')"
        },
        "min": {
            "type": "moment.Moment | moment.Duration",
            "default": "moment().hours(0).minutes(0) | moment.duration('0:0')"
        },
        "max": {
            "type": "moment.Moment | moment.Duration",
            "default": "moment().hours(23).minutes(59) | moment.duration('4:0')"
        },
        "step": {
            "type": "number",
            "default": 5
        },
        "format": {
            "type": "string",
            "values": "see Moment.js formating options",
            "default": "LT"
        },
        "disabled": {
            "type": "boolean",
            "default": false
        },
        "waiting": {
            "type": "boolean",
            "default": false
        },
        "error": {
            "type": "boolean",
            "default": false
        },
        "valid": {
            "type": "boolean",
            "default": false
        },
        "errorMessage": {
            "type": "string"
        },
        "validMessage": {
            "type": "string"
        },
        "helperMessage": {
            "type": "string"
        }
    },
    "overview": "timepicker.overview",
    "preview": "timerpicker.preview"
}
