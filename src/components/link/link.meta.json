{
    "tag": "m-link",
    "name": "m-link-meta:name",
    "production": true,
    "attributes": {
        "url" : {
            "type": "string",
            "default": "/"
        },
        "mode": {
            "type": "string",
            "values": [
                "router-link",
                "link",
                "button"
            ],
            "default": 0
        },
        "disabled" : {
            "type": "boolean",
            "default": false
        },
        "unvisited" : {
            "type": "boolean",
            "default": false
        },
        "underline" : {
            "type": "boolean",
            "default": true
        },
        "icon" : {
            "type": "boolean",
            "default": false
        },
        "iconName" : {
            "type": "string"
        },
        "iconPosition" : {
            "type": "string",
            "values": [
                "left",
                "right"
            ],
            "default": 0
        },
        "iconSize" : {
            "type": "string",
            "default": "12px"
        }
    },
    "overview": "link.overview",
    "preview": "link.preview"
}
