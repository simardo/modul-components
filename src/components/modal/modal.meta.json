{
    "tag": "m-modal",
    "name": "m-modal-meta:name",
    "production": true,
    "attributes": {
        "title": {
            "type": "string"
        },
        "message": {
            "type": "string"
        },
        "okLabel": {
            "type": "string | undefined"
        },
        "cancelLabel": {
            "type": "string | undefined"
        },
        "openTrigger": {
            "type": "string",
            "value": [
                "click",
                "hover",
                "manual"
            ],
            "default": 0
        },
        "open": {
            "type": "boolean",
            "default": false
        },
        "id": {
            "type": "string",
            "default": "mPortal"
        },
        "disabled": {
            "type": "boolean",
            "default": false
        },
        "trigger": {
            "type": "HTMLElement"
        }
    },
    "overview": "modal.overview",
    "preview": "modal.preview"
}
