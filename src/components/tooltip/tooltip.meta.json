{
    "tag": "m-tooltip",
    "name": "m-tooltip-meta:name",
    "production": false,
    "attributes": {
        "open": {
            "type": "boolean",
            "default": false
        },
        "mode": {
            "type": "string",
            "values": [
                "icon",
                "link"
            ],
            "default": "0"
        },
        "placement": {
            "type": "MPopperPlacement",
            "values": [
                "top",
                "top-start",
                "top-end",
                "right",
                "right-start",
                "right-end",
                "bottom",
                "bottom-start",
                "bottom-end",
                "left",
                "left-start",
                "left-end"
            ],
            "default": 6
        },
        "closeButton": {
            "type": "boolean",
            "values": [
                "true",
                "false"
            ],
            "default": "0"
        },
        "classNamePortalTarget": {
            "type": "string"
        },
        "disabled": {
            "type": "boolean",
            "default": false
        }
    },
    "overview": "tooltip.overview",
    "preview": "tooltip.preview"
}
