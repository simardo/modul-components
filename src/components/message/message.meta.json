{
    "tag": "m-message",
    "name": "m-message-meta:name",
    "production": true,
    "attributes": {
        "state": {
            "type": "string",
            "values": [
                "error",
                "warning",
                "information",
                "success"
            ],
            "default": 4
        },
        "skin": {
            "type": "string",
            "values": [
                "light",
                "regular"
            ],
            "default": 1
        },
        "icon": {
            "type": "boolean",
            "default": true
        },
        "closeButton": {
            "type": "boolean",
            "default": true
        },
        "visible": {
            "type": "boolean",
            "default": false
        }
    },
    "overview": "message.overview",
    "preview": "message.preview"
}
