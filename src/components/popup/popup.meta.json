{
    "tag": "m-popup",
    "name": "m-popup-meta:name",
    "production": false,
    "attributes": {
        "open": {
            "type": "boolean"
        },
        "placement": {
            "type": "MPopperPlacement",
            "values": [
                "top",
                "top-start",
                "top-end",
                "right",
                "right-start",
                "right-end",
                "bottom",
                "bottom-start",
                "bottom-end",
                "left",
                "left-start",
                "left-end"
            ],
            "default": 6
        },
        "openTrigger": {
            "type": "MOpenTrigger",
            "default": "Click"
        },
        "closeOnBackdrop": {
            "type": "boolean",
            "default": true
        },
        "focusManagement": {
            "type": "boolean",
            "default": true
        },
        "width": {
            "type": "string",
            "default": "auto"
        },
        "id": {
            "type": "string"
        },
        "disable": {
            "type": "boolean",
            "default": false
        },
        "shadow": {
            "type": "boolean",
            "default": true
        },
        "padding": {
            "type": "boolean",
            "default": true
        },
        "paddingHeader": {
            "type": "boolean",
            "default": true
        },
        "paddingBody": {
            "type": "boolean",
            "default": true
        },
        "paddingFooter": {
            "type": "boolean",
            "default": true
        },
        "beforeEnter": {
            "type": "any"
        },
        "enter": {
            "type": "any"
        },
        "afterEnter": {
            "type": "any"
        },
        "enterCancelled": {
            "type": "any"
        },
        "beforeLeave": {
            "type": "any"
        },
        "leave": {
            "type": "any"
        },
        "afterLeave": {
            "type": "any"
        },
        "leaveCancelled": {
            "type": "any"
        }
    },
    "overview": "popup.overview",
    "preview": "popup.preview"
}
