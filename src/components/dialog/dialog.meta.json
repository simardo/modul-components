{
    "tag": "m-dialog",
    "name": "m-dialog-meta:name",
    "production": true,
    "attributes": {
        "size": {
            "type": "string",
            "value": [
                "full-size",
                "large",
                "default",
                "small"
            ],
            "default": 2
        },
        "closeOnBackdrop": {
            "type": "boolean",
            "default": true
        },
        "title": {
            "type": "string"
        },
        "padding": {
            "type": "boolean",
            "default": true
        },
        "paddingHeader": {
            "type": "boolean",
            "default": true
        },
        "paddingBody": {
            "type": "boolean",
            "default": true
        },
        "paddingFooter": {
            "type": "boolean",
            "default": true
        },
        "openTrigger": {
            "type": "string",
            "value": [
                "click",
                "hover",
                "manual"
            ],
            "default": 0
        },
        "open": {
            "type": "boolean",
            "default": false
        },
        "id": {
            "type": "string",
            "default": "mPortal"
        },
        "disabled": {
            "type": "boolean",
            "default": false
        },
        "trigger": {
            "type": "HTMLElement"
        }
    },
    "overview": "dialog.overview",
    "preview": "dialog.preview"
}
